{
  "name": "users",
  "version": "1.0.0",
  "description": "CatMatch Users MicroService",
  "license": "MIT",
  "author": "jj-int-dev",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "build": "esbuild src/server.ts --bundle --platform=node --format=esm --packages=external --outfile=dist/server.js",
    "start": "node dist/server.js",
    "dev": "tsx --watch src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull --config=drizzle-introspect.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "db:up": "drizzle-kit up",
    "db:drop": "drizzle-kit drop"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "install": "^0.13.0",
    "npm": "^11.5.2",
    "prettier": "^3.6.2",
    "supertest": "^7.2.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.0",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "esbuild": "^0.27.3",
    "multer": "^2.0.2",
    "postgres": "^3.4.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.5"
  }
}
